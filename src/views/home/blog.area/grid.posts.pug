//-- Single Post
.col-12.col-md-6
	.single-post.wow.fadeInUp(data-wow-delay='.4s')
		//-- Post Thumb
		.post-thumb
			img(src='/img/blog-img/2.jpg' alt='')
		//-- Post Content
		.post-content
			.post-meta.d-flex
				.post-author-date-area.d-flex
					//-- Post Author
					.post-author
						a(href='#') By Marian
					//-- Post Date
					.post-date
						a(href='#') May 19, 2017
				//-- Post Comment & Share Area
				.post-comment-share-area.d-flex
					//-- Post Favourite
					.post-favourite
						a(href='#')
							i.fa.fa-heart-o(aria-hidden='true')
							|  10
					//-- Post Comments
					.post-comments
						a(href='#')
							i.fa.fa-comment-o(aria-hidden='true')
							|  12
					//-- Post Share
					.post-share
						a(href='#')
							i.fa.fa-share-alt(aria-hidden='true')
			a(href='#')
				h4.post-headline Where To Get The Best Sunday Roast In The Cotswolds
