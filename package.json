{
	"name": "shibamiandme-blog",
	"version": "1.0.0",
	"description": "Blog application built using @yggdrasilts architecture and 'pug' view template.",
	"license": "MIT",
	"author": "David Lopez",
	"scripts": {
		"clean": "gulp clean",
		"clean:all": "gulp clean:all",
		"compile": "gulp build",
		"compile:test": "gulp build:test",
		"start": "gulp start",
		"test": "gulp compile:test && nyc --require ts-node/register mocha --exit --recursive \"src/**/*.spec.ts\"",
		"postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'bower_components', 'junction') } catch (e) { }\""
	},
	"dependencies": {
		"@yggdrasilts/core": "canary",
		"@yggdrasilts/data": "canary",
		"@yggdrasilts/mvc": "canary",
		"lodash": "^4.17.5",
		"moment": "^2.20.1",
		"pug": "^2.0.0-rc.4",
		"quill": "^1.1.6",
		"@bower_components/KaTeX": "Khan/KaTeX#^0.6.0",
		"@bower_components/highlightjs": "components/highlightjs#^9.6.0",
		"@bower_components/polymer": "Polymer/polymer#^1.4.0",
		"@bower_components/quill-element": "dmonad/quill-element#^0.2.1",
		"@bower_components/webcomponentsjs": "webcomponents/webcomponentsjs#^0.7.24"
	},
	"devDependencies": {
		"@yggdrasilts/devs": "canary",
		"@yggdrasilts/testing": "canary"
	}
}
